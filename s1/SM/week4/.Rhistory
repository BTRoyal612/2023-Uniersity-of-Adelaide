}
y_bar = y_bar/y_len
Sxx = 0
for (i in (1:x_len)) {
Sxx = Sxx + (x[i] - x_bar)^2
}
Syy = 0
for (i in (1:x_len)) {
Syy = Syy + (y[i] - y_bar)^2
}
Sxy = 0
for (i in (1:x_len)) {
Sxy = Sxy + (x[i] - x_bar)*(y[i] - y_bar)
}
B1 = Sxy/Sxx
n = x_len
Se_2 = (1/(n-2))*(Syy-B1^2*Sxx)
t = (B1)/(sqrt(Se_2/Sxx))
